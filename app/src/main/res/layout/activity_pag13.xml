<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".pag13">

    <ImageView
        android:id="@+id/imageView26"
        android:layout_width="248dp"
        android:layout_height="272dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="36dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/stemma" />

    <ImageView
        android:id="@+id/imageView27"
        android:layout_width="1083dp"
        android:layout_height="1032dp"
        android:layout_marginStart="340dp"
        android:layout_marginBottom="1452dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/imm13" />

    <TextView
        android:id="@+id/textView41"
        android:layout_width="1263dp"
        android:layout_height="271dp"
        android:layout_marginStart="88dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="1156dp"
        android:text="@string/facciata"
        android:textAlignment="center"
        android:textColor="@android:color/black"
        android:textSize="100sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView42"
        android:layout_width="1083dp"
        android:layout_height="911dp"
        android:layout_marginStart="352dp"
        android:layout_marginBottom="172dp"
        android:text="@string/testo8"
        android:textAlignment="viewStart"
        android:textColor="@color/grigioscuro"
        android:textSize="65sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <View
        android:id="@+id/view22"
        android:layout_width="390dp"
        android:layout_height="2dp"
        android:layout_marginTop="199dp"
        android:layout_marginBottom="954dp"
        android:background="@android:color/darker_gray"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView42"
        app:layout_constraintTop_toBottomOf="@+id/textView41" />

    <ImageView
        android:id="@+id/imageView28"
        android:layout_width="364dp"
        android:layout_height="579dp"
        android:layout_marginEnd="1112dp"
        android:layout_marginBottom="540dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/arancione_no" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/backbtnpag13"
        android:layout_width="0dp"
        android:layout_height="68dp"
        android:layout_marginTop="72dp"
        android:layout_marginEnd="1244dp"
        android:layout_marginBottom="96dp"
        android:background="@color/blunotte"
        android:clickable="true"
        android:tint="@color/colorAccent"

        app:backgroundTint="@color/arancio"
        app:fabCustomSize="68dp"
        app:fabSize="auto"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:maxImageSize="34dp"
        app:srcCompat="@drawable/arrowleftmedia" />

</android.support.constraint.ConstraintLayout>